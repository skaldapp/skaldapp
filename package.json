{
  "$schema": "https://www.schemastore.org/package",
  "name": "skaldapp",
  "version": "4.0.48",
  "description": "Skald is an open-source browser-based Vue.js web editor that allows creating static websites with Vue SFCs without Node.js setup. Features runtime SFC compilation, Monaco editor, WYSIWYG mode, Tailwind CSS, and multiple storage options.",
  "keywords": [
    "vuejs",
    "web-editor",
    "browser-based",
    "open-source",
    "sfc",
    "single-file-components",
    "runtime-compilation",
    "monaco-editor",
    "wysiwyg",
    "tailwind-css",
    "unocss",
    "quasar",
    "typescript",
    "vite",
    "static-websites",
    "no-setup",
    "development-environment",
    "electron",
    "cross-platform",
    "i18n"
  ],
  "license": "AGPL-3.0-or-later",
  "author": "Jerry Bruwes <jbruwes@gmail.com> (https://jbruwes.github.io)",
  "type": "module",
  "scripts": {
    "build": "quasar build",
    "build:electron": "quasar build -m electron",
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "postinstall": "quasar prepare",
    "lint": "eslint ."
  },
  "prettier": "@skaldapp/configs/prettierrc",
  "dependencies": {
    "@ai-sdk/mistral": "^3.0.19",
    "@ai-sdk/vue": "^3.0.77",
    "@aws-sdk/client-s3": "^3.985.0",
    "@electron/remote": "^2.1.3",
    "@iconify/vue": "^5.0.0",
    "@milkdown/crepe": "^7.18.0",
    "@milkdown/kit": "^7.18.0",
    "@milkdown/plugin-emoji": "^7.18.0",
    "@milkdown/utils": "^7.18.0",
    "@milkdown/vue": "^7.18.0",
    "@quasar/app-vite": "^2.4.0",
    "@skaldapp/fsa": "^1.0.34",
    "@skaldapp/monaco-sfc": "^1.1.7",
    "@skaldapp/shared": "^1.2.37",
    "@smithy/fetch-http-handler": "^5.3.9",
    "@unhead/vue": "^2.1.3",
    "@vueuse/core": "^14.2.0",
    "ai": "^6.0.77",
    "crypto-es": "^3.1.2",
    "globals": "^17.3.0",
    "hexo-front-matter": "^5.0.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-deflist": "^3.0.0",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-ins": "^4.0.0",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "mime": "^4.1.0",
    "monaco-editor": "0.52.2",
    "monaco-tailwind": "^0.1.0-alpha.2",
    "monacopilot": "^1.2.12",
    "path-browserify": "^1.0.1",
    "pinia": "^3.0.4",
    "pinia-plugin-store": "^3.0.7",
    "prismjs": "^1.30.0",
    "prosemirror-view": "^1.41.6",
    "quasar": "^2.18.6",
    "unhead": "^2.1.3",
    "vue": "^3.5.27",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@fontsource/noto-sans": "^5.2.10",
    "@fontsource/noto-serif": "^5.2.9",
    "@fontsource/space-mono": "^5.2.9",
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@quasar/extras": "^1.17.0",
    "@quasar/quasar-app-extension-qmarkdown": "^2.0.5",
    "@skaldapp/configs": "^1.2.53",
    "@skaldapp/runtime": "^3.0.33",
    "@types/markdown-it-emoji": "^3.0.1",
    "@types/markdown-it-footnote": "^3.0.4",
    "@types/path-browserify": "^1.0.3",
    "@types/prismjs": "^1.26.5",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@unocss/vite": "^66.6.0",
    "autoprefixer": "^10.4.24",
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0",
    "eslint": "^9.39.2",
    "nord": "^0.2.1",
    "vite": "^7.3.1",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-static-copy": "^3.2.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "last 4 Chrome versions",
          "last 4 Firefox versions",
          "last 4 Edge versions",
          "last 4 Safari versions",
          "last 4 Android versions",
          "last 4 ChromeAndroid versions",
          "last 4 FirefoxAndroid versions",
          "last 4 iOS versions"
        ]
      }
    }
  },
  "productName": "Skald"
}
